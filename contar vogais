#include <stdio.h>
#include <stdlib.h>
#include <string.h>
///contar quantas vogais em uma frase
int main()
{
    char vogais[100];

    puts("Digite uma frase: ");
    gets(vogais);

    int i, cont=0;

    for(i=0; vogais[i] != '\0'; i++)
    {
        if(vogais[i]=='a' || vogais[i]=='A' || vogais[i]=='e' || vogais[i]=='E'|| vogais[i]=='I' || vogais[i]=='i' || vogais[i]=='o' || vogais[i]=='O'|| vogais[i]=='u'|| vogais[i]=='U')
        {
            cont++;
        }

    }
    printf("%d vogais", cont);
    return 0;
}
